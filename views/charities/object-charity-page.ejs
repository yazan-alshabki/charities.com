<body id="page-top">
    <%- include('../partials/navbar.ejs') -%>

  <div class="shadow"></div>
  <header class="first text-center">
    
    <div class="container">
      <img class="d-block mx-auto mb-5 logo w-50" src="/uploads/<%= charity.logo %>" />
