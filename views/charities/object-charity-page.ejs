        <p class="fs-5 mt-2">
          <%= charity.description  %>
        </p>
      </div>
    </div>
  </section>
<section>
  <div class="container text-center pt-5">
    <h2 class="text-uppercase mt-5 mb-5 text-purple">Our Services</h2> 
